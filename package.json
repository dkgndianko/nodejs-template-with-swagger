{
    "name": "app-test-api",
    "version": "1.0.0",
    "description": "This is an API for Testing",
    "main": "src/index.ts",
    "scripts": {
        "start:dev": "nodemon",
        "dev": "concurrently -k -n \"Typescript,Node\" -p \"[{name}]\" -c \"blue,green\" \"tsc --watch\" \"nodemon dist/index.js\"",
        "build": "rimraf ./dist && tsc",
        "start": "strict-npm-engines && npm run build && node dist/index.js",
        "lint": "eslint . --ext .ts",
        "test": "NODE_ENV=test jest --verbose",
        "test-watch": "jest NODE_ENV=test --verbose --watchAll",
        "ci": "npm run lint && npm run test"
    },
    "keywords": [
        "API",
        "Test API",
        "Swagger"
    ],
    "author": "dev@ctsfares.com",
    "license": "ISC",
    "devDependencies": {
        "@types/express": "^4.17.3",
        "@types/express-unless": "^0.5.1",
        "@types/jest": "^25.1.4",
        "@types/jsonwebtoken": "^8.3.9",
        "@types/mongoose": "^5.7.6",
        "@types/morgan": "^1.7.35",
        "@types/node": "^13.9.1",
        "@types/swagger-ui-express": "^4.1.2",
        "@typescript-eslint/eslint-plugin": "^2.24.0",
        "@typescript-eslint/parser": "^2.24.0",
        "concurrently": "^5.1.0",
        "eslint": "^6.8.0",
        "jest": "^25.1.0",
        "nodemon": "^2.0.2",
        "rimraf": "^3.0.2",
        "strict-npm-engines": "0.0.1",
        "ts-jest": "^25.2.1",
        "ts-node": "^8.6.2",
        "tslint": "^6.1.0",
        "typescript": "^3.8.3",
        "mongodb-memory-server": "^6.4.1"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "dotenv": "^8.2.0",
        "envalid": "^6.0.1",
        "express": "^4.17.1",
        "express-unless": "^0.5.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.9.5",
        "morgan": "^1.9.1",
        "swagger-ui": "^3.25.0",
        "swagger-ui-express": "^4.1.4"
    },
    "engines": {
        "node": ">=12.9.0",
        "npm": ">=6.12.1"
    }
}
